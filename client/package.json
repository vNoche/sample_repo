{
  "name": "client",
  "version": "15.7.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "export": "next build && next export",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "start": "set HTTPS=true&&set SSL_CRT_FILE={./etc/ca.crt}&&set SSL_KEY_FILE={./etc/ca.key}&&node server.js",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "changelog": "node ./project_utils/generateChangelog.js && node ./project_utils/changelog-table.js",
    "changelog:release-notes": "node ./project_utils/generateChangelog.js && node ./project_utils/changelog-release-notes-table.js",
    "suggest-release-version": "node ./project_utils/generateChangelog.js && node ./releaseVersion.js",
    "typecheck": "tsc --noEmit --project './tsconfig.json'",
    "typecheck:single": "tsc --noEmit",
    "translate:rebuild": "bash scripts/rebuildTranslations.sh",
    "translate:toCsv": "bash scripts/makeCsv.sh",
    "translate:toJson": "bash scripts/makeJson.sh",
    "translate:toDist": "cp -r public/locales dist && npx minify-json dist/locales",
    "docker:build": "docker build -t alta/${npm_package_name}:${npm_package_version}_dev . -f docker/Dockerfile.dev",
    "docker:build:prod": "docker build -t alta/${npm_package_name}:${npm_package_version} . -f docker/prod.Dockerfile",
    "docker:run": "docker run -p 3000:3000 --env-file ../.env alta/${npm_package_name}:${npm_package_version}_dev",
    "docker:push": "docker push alta/${npm_package_name}:${npm_package_version}_dev",
    "docker:push:prod": "docker push alta/${npm_package_name}:${npm_package_version}",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx}' 'tests/jest/**/*.{js,ts}'",
    "prettier:no-quotes": "prettier --write src/**/*.{js,ts,tsx} tests/jest/**/*.{js,ts}",
    "ssl": "env PROTOCOL_TO_USE=https node server.js",
    "up-deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "aos": "2.3.4",
    "array-to-tree": "2.2.0",
    "axios": "0.27.2",
    "card-validator": "8.1.1",
    "classnames": "2.3.1",
    "color": "3.1.3",
    "current-device": "0.10.2",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-request-id": "1.4.1",
    "express-winston": "4.2.0",
    "global": "4.4.0",
    "html-react-parser": "1.4.12",
    "http-proxy-middleware": "1.3.0",
    "inobounce": "0.2.1",
    "next": "12.0.10",
    "next-i18next": "7.0.1",
    "object-fit-images": "3.2.4",
    "plyr": "3.7.2",
    "pm2": "4.5.6",
    "puppeteer": "19.2.2",
    "qs": "6.10.3"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "typescript": "4.6.4",
    "yargs": "17.5.1",
    "git-log-as-object": "2.0.0"
  }
}
